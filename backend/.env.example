# MedFin Backend Configuration
# Copy this file to .env and update with your secure values

# Security
SECRET_KEY=your-super-secure-secret-key-change-this-in-production-use-at-least-32-characters
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database
DATABASE_URL=sqlite+aiosqlite:///./medfin.db

# CORS (Add your frontend URLs)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Application
APP_NAME=MedFin
API_VERSION=v1
